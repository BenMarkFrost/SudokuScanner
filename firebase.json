{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [ 
      {
      "source": "**/*",
      "headers": [ {
        "key": "Access-Control-Allow-Origin",
        "value": "*"
      } ]
      } 
    ],
    "rewrites": [
      {
      "source": "/",
      "destination":"/templates/index.html"
      }
      // {
      // "source": "**",
      //   "run": {
      //   "serviceId": "sudokuapp",
      //   "region": "europe-west1"
      //   }
      // }
    ]
  }
}