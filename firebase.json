{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
        "source": "/frame",
          "run": {
          "serviceId": "sudokuapp",
          "region": "europe-west1"
          }
        }, 
        {
          "source": "**", 
          "destination": "templates/index.html"
        }
    ]
  }
}