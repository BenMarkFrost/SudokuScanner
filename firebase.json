{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
      "source": "/",
      "destination":"/templates/index.html"
      },
      {
      "source": "**",
        "run": {
        "serviceId": "sudokuapp",
        "region": "europe-west1"
        }
      }
    ]
  }
}