{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "/",
        "destination": "public/templates/index.html"},
        {
        "source": "**",
          "run": {
          "serviceId": "sudokuapp"
          }
        }
    ]
  }
}