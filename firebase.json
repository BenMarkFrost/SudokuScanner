{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
        {
        "source": "**",
          "run": {
          "serviceId": "sudokuapp",
          "region": "europe-west1"
          }
        }
    ]
  }
}